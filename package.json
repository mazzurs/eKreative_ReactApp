{
  "name": "react-start-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ --watch --recursive",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts build",
    "test": "standard --verbose | snazzy && react-scripts test --env=jsdom",
    "fix": "standard --verbose --fix | snazzy",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "google-maps-react": "^1.1.2",
    "immutability-helper": "^2.2.0",
    "react": "^16.0.0",
    "react-anything-sortable": "^1.7.3",
    "react-dom": "^16.0.0",
    "react-drag-list": "^1.0.4",
    "react-drag-sortable": "^1.0.5",
    "react-redux": "^5.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "react-sortable-hoc": "^0.6.8",
    "react-sortable-list": "^1.0.3",
    "redux": "^3.6.0",
    "redux-persist": "^4.7.1",
    "redux-thunk": "^2.2.0",
    "sortablejs": "^1.7.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "^0.9.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  }
}
